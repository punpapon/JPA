<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="1.0" xmlns="http://java.sun.com/xml/ns/persistence/orm">

	<named-query name="User.deleteUserOperatorByUserId">
		<query>DELETE UserOperatorEntity o WHERE o.user.userId = :userId</query>
	</named-query>
	
	<named-query name="User.deleteUserGroupByUserId">
		<query>DELETE UserGroupEntity o WHERE o.user.userId = :userId</query>
	</named-query>

</entity-mappings>